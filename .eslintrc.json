{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "amd": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "semi": ["error","always"],
        "eqeqeq" : ["error","always"],
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "comma-dangle": ["error", "only-multiline"],
        "no-console": 0
    }
}
